"line":100,"column":45},"source":"src/components/spot-it/SpotIt.js","original":{"line":7,"column":18}},{"generated":{"line":100,"column":47},"source":"src/components/spot-it/SpotIt.js","original":{"line":7,"column":4}},{"generated":{"line":101,"column":0},"source":"src/components/spot-it/SpotIt.js","original":{"line":8,"column":4}},{"name":"state","generated":{"line":101,"column":10},"source":"src/components/spot-it/SpotIt.js","original":{"line":8,"column":9}},{"generated":{"line":101,"column":15},"source":"src/components/spot-it/SpotIt.js","original":{"line":8,"column":4}},{"generated":{"line":101,"column":18},"source":"src/components/spot-it/SpotIt.js","original":{"line":8,"column":17}},{"name":"engine","generated":{"line":101,"column":24},"source":"src/components/spot-it/SpotIt.js","original":{"line":8,"column":22}},{"generated":{"line":101,"column":30},"source":"src/components/spot-it/SpotIt.js","original":{"line":8,"column":17}},{"name":"getState","generated":{"line":101,"column":31},"source":"src/components/spot-it/SpotIt.js","original":{"line":8,"column":29}},{"generated":{"line":101,"column":39},"source":"src/components/spot-it/SpotIt.js","original":{"line":8,"column":17}},{"generated":{"line":101,"column":41},"source":"src/components/spot-it/SpotIt.js","original":{"line":8,"column":4}},{"generated":{"line":102,"column":0},"source":"src/components/spot-it/SpotIt.js","original":{"line":5,"column":21}},{"generated":{"line":103,"column":0},"source":"src/components/spot-it/SpotIt.js","original":{"line":9,"column":3}},{"name":"Component","generated":{"line":106,"column":2},"source":"src/components/spot-it/SpotIt.js","original":{"line":4,"column":36}},{"name":"SpotIt","generated":{"line":108,"column":18},"source":"src/components/spot-it/SpotIt.js","original":{"line":4,"column":21}}],"sources":{"src/components/spot-it/SpotIt.js":"import React, {Component, Fragment} from 'react';\nimport SpotItEngine from './spot-it-engine';\n\nexport default class SpotIt extends Component {\n  constructor(props) {\n    super(props);\n    this.engine = new SpotItEngine();\n    this.state = this.engine.getState();\n  }\n\n  handleClick = (symbol) => {\n    this.engine.isMatch(symbol);\n    this.setState(this.engine.getState());\n  }\n\n  handleImgClick = (ev) => {\n    // notice that this gives values close to zero when you\n    // click in the top-left corner of EACH image,\n    // and values close to 1.0 when you click in the bottom-right\n    // corner of EACH image.\n    let percentX = ev.nativeEvent.offsetX / ev.target.width;\n    let percentY = ev.nativeEvent.offsetY / ev.target.height;\n    console.log('x%', percentX, 'y%', percentY);\n\n    // TODO: attach a new function to the engine similar to isMatch\n    // that accepts percentage coordinates, finds the closest symbol\n    // for the current cards, gets the symbol, then passes that symbol\n    // to the original handleClick function.\n  }\n\n  render = () => {\n    return <Fragment>\n      <div>\n        Guesses Left: {this.state.guessesLeft}\n      </div>\n      <div>\n        <img onClick={this.handleImgClick} \n            src={this.state.card1Image} />\n        <img onClick={this.handleImgClick} \n            src={this.state.card2Image} />\n      </div>\n      <div>\n        Card 1: {this.state.card1.map((symbol, i) => {\n          return <button key={i} onClick={() => this.handleClick(symbol)}>{symbol}</button>\n        })}\n      </div>\n      <div>\n        Card 2: {this.state.card2.map((symbol, i) => {\n          return <button key={i} onClick={() => this.handleClick(symbol)}>{symbol}</button>\n        })}\n      </div>\n    </Fragment>\n  }\n}"},"lineCount":null}},"hash":"4c3acfe69c786b9712be0544bef8095c","cacheData":{"env":{}}}